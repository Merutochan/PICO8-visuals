pico-8 cartridge // http://www.pico-8.com
version 19
__lua__
n=16
cl={7,8,9,10,11,12,13,14,15}
txt={'s','h','i','n'}
function _init()
	ax=40
	ay=40
	bx=96
	by=96
	vx=1
	vy=1
 cx=68
 cy=68
 l={}
 for i = 1, n do
 	add(l, po(i))
 end
end


function _update60()
	
	for i=1,n do
		c=cos(time()/2+i/n)
		s=sin(time()/3+i/n)
		r = 32
 	r/= sqrt(2)
 	l[i].x=r*c+r*s +cx
 	l[i].y=r*s-r*c +cy
	end
	
	if btn(2) then
		vx+=0.01
	elseif btn(3) then
		vx-=0.01
	end
	if btn(1) then
		vy+=0.01
	elseif btn(0) then
		vy-=0.01
	end
	ax += cos(time()) 
	ay += sin(time())
	bx -= cos(time())
	by -= sin(time())
	
	cx = 64+ 12*cos(time()/7)
	cy = 64+ 12*sin(time()/5)
end


function _draw()
	cls()

	--background
	for i=0, 16 do
		for j=0, 16 do
 		x=(i*8+time()*vx*2)%136-8
 		y=(j*8+time()*vy*2)%136-8
 		-- pattern mov
 		v=0
 		v+=j*vx
 		v+=i*vy
 		
 		spr(flr(time()*10+v)%24,x,y)
		end
	end
	
	-- 
	if btn(4) then
		rectfill(ax,ay,bx,by,0)
	end

end

function po(i)
	p = {}
	p.x = cx + (i)*4
	p.y = cy + (i)*4
	return p
end
