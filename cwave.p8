-- edited a zep demo

r=64
n=100

function _update60()
	f1=4*sin(time()/7)
	f2=cos(time()/7)
	r1=5
	r2=5
	t=time()
	if btn(2) then n+=0.1 end
	if btn(3) then n-=0.1 end
end

function _draw()
	cls()
	for y=-r,r,r1 do
		for x=-r,r,r2 do
			local dist=sqrt(x*x+y*y)
			z=cos(dist/n-t)*f1
			xx=(r+x-z*4*cos(t/2))
			yy=(r+y+z*4*sin(t/3))
			xxx=(r+x-z*4)
			yyy=(r+y+z*4)
			line(xx,yy,xxx,yyy,(z/5)+t)
		end
	end

end

