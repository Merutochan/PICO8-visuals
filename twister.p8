version 19
__lua__
function _init()
ni=4
size=16
twst=1000
lst = {0,5,7}
cnt = 0
c=64
lin=true
end

function _update60()
	
	if btn(0) then twst-=d(twst) end
	if btn(1) then twst+=d(twst) end
	if btnp(2) then size+=1 end
	if btnp(3) then size-=1 end
	if btnp(4) then cnt+=1 poke(0x5f2c, lst[(cnt%3)+1]) end
	if btnp(5) then lin= not lin end
	if (cnt%3==0) then c=64 else c=32 end
end

function _draw()
	cls()
	
--	pal(1,14)
	
	for o=0,16 do
		for p=0,16 do
			pal(1,(o+p)/3%14+1)
			x=(o*8+time()*20)%136-8
			y=(p*8+time()*20)%136-8
			sspr(0,0,8,8,x,y)
			pal(1,1)
		end
	end
--	pal(0,0)
--	pal(1,1)
	
	b=16*sin(time())
	for i=0,ni-1 do	
		for x=0,127 do
			a = time()+i/ni+sin(time()/32+x/twst)
			y1=c+size*sin(a)-b
			y2=c+size*sin(a+1/(ni))-b
			if y1<y2 then
				if lin then
					line(y1,x,y2,x,8+i+a)
				else
					if x%8==0 then
						rectfill(y1,x,y2,x+8,8+i+a)
					end
				end
			end
		end
	end
	color(7)
end

function d(a)
	if abs(a)<10 then
		return 0.1
	elseif abs(a)<100 then
		return 1
	elseif abs(a)<3000 then
		return 10
	else
		return 50	
	end
end
